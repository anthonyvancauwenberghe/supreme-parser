{
    "name": "deviouspk/supreme-parser",
    "description": "",
    "keywords": [
        "supreme",
        "parser"
    ],
    "homepage": "https://github.com/deviouspk/supreme-parser",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthony Vancauwenberghe",
            "email": "anthony.vanc@gmail.com",
            "homepage": "https://github.com/larapie",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2",
        "guzzlehttp/guzzle": "^6.3",
        "paquettg/php-html-parser": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Supreme\\Parser\\": "src"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },

    "autoload-dev": {
        "psr-4": {
            "Supreme\\Parser\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
